[time]
command=date '+%I : %M : %S    %m / %d / %y                                                                                             ' 
interval=1

[pomodoro]
command=~/.config/i3blocks/scripts/pomodoro.sh
interval=1
markup=none

[cpu]
command=~/.config/i3blocks/scripts/cpu.sh
interval=5
color=#ffbf7f

[memory]
command=~/.config/i3blocks/scripts/memory.sh
interval=5
color=#ffbf7f

[volume]
command=~/.config/i3blocks/scripts/volume_icon.sh~
interval=2

[wifi]
command=~/.config/i3blocks/scripts/wifi.sh
interval=5

[bluetooth]
command=~/.config/i3blocks/scripts/bluetooth.sh
interval=5
